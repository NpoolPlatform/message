    "v1OrderState": {
      "type": "string",
      "enum": [
        "DefaultOrderState",
        "OrderStateWaitPayment",
        "OrderStateCheckPayment",
        "OrderStatePaymentTransferReceived",
        "OrderStatePaymentTransferReceivedCheck",
        "OrderStatePaymentTransferBookKept",
        "OrderStatePaymentTransferBookKeptCheck",
        "OrderStatePaymentBalanceSpent",
        "OrderStatePaymentBalanceSpentCheck",
        "OrderStateGoodStockTransferred",
        "OrderStateGoodStockTransferredCheck",
        "OrderStateCommissionAdded",
        "OrderStateCommissionAddedCheck",
        "OrderStateAchievementBookKept",
        "OrderStateAchievementBookKeptCheck",
        "OrderStatePaid",
        "OrderStateInService",
        "OrderStatePaymentTimeout",
        "OrderStatePreCancel",
        "OrderStatePreCancelCheck",
        "OrderStatePreExpired",
        "OrderStatePreExpiredCheck",
        "OrderStateRestoreExpiredStock",
        "OrderStateRestoreExpiredStockCheck",
        "OrderStateRestoreCanceledStock",
        "OrderStateRestoreCanceledStockCheck",
        "OrderStateReturnCanceledBalance",
        "OrderStateReturnCanceledBalanceCheck",
        "OrderStateCanceled",
        "OrderStateExpired"
      ],
      "default": "DefaultOrderState",
      "description": "-\u003e OrderStateCheckPayment\n - OrderStateCheckPayment: -\u003e OrderStatePaymentTimeout || PaymentTransferReceived\n - OrderStatePaymentTransferReceived: Payment process\n\n-\u003e OrderStatePaymentTransferReceivedCheck (received on blockchain)\n - OrderStatePaymentTransferReceivedCheck: -\u003e OrderStatePaymentTransferBookKept (bookkeeping)\n - OrderStatePaymentTransferBookKept: -\u003e OrderStatePaymentTransferBookKeptCheck\n - OrderStatePaymentTransferBookKeptCheck: -\u003e OrderStatePaymentBalanceSpent (spend locked balance)\n - OrderStatePaymentBalanceSpent: -\u003e OrderStatePaymentBalanceSpentCheck\n - OrderStatePaymentBalanceSpentCheck: -\u003e OrderStateGoodStockTransferred (transfer stock from locked to wait start)\n - OrderStateGoodStockTransferred: -\u003e OrderStateGoodStockTransferredCheck\n - OrderStateGoodStockTransferredCheck: -\u003e OrderStateCommissionAdded (commission added)\n - OrderStateCommissionAdded: -\u003e OrderStateCommissionAddedCheck\n - OrderStateCommissionAddedCheck: -\u003e OrderStateAchievementBookKept\n - OrderStateAchievementBookKept: -\u003e OrderStateAchievementBookKeptCheck\n - OrderStateAchievementBookKeptCheck: -\u003e OrderStatePaid\n - OrderStatePaid: Paid\n - OrderStatePaymentTimeout: Timeout of order\n\n-\u003e OrderStatePreCancel\n - OrderStatePreCancel: End or order\n\n-\u003e OrderStatePreCancelCheck\n - OrderStatePreCancelCheck: -\u003e OrderStateRestoreCanceledStock\n - OrderStatePreExpired: -\u003e OrderStatePreExpired\n - OrderStatePreExpiredCheck: -\u003e OrderStateRestoreExpiredStock\n - OrderStateRestoreExpiredStock: Calculation\n\n-\u003e OrderStateRestoreExpiredStockCheck\n - OrderStateRestoreExpiredStockCheck: -\u003e OrderStateExpired\n - OrderStateRestoreCanceledStock: -\u003e OrderStateRestoreCanceledStockCheck\n - OrderStateRestoreCanceledStockCheck: -\u003e OrderStateReturnCanceledBalance\n - OrderStateReturnCanceledBalance: -\u003e OrderStateReturnCanceledBalanceCheck\n - OrderStateReturnCanceledBalanceCheck: -\u003e OrderStateCanceled\n - OrderStateCanceled: End state",
      "title": "- OrderStateWaitPayment: Common state"
    },
